{
  "compilerOptions": {
    "experimentalDecorators": true,
    "allowJs": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "outDir": "../dist_server",
    "rootDir": "./",
    "lib": ["es2017"],
    "sourceMap": true,
    "target": "es5",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "typeRoots": ["./server/typings", "./node_modules/@types"]
  },
  "include": ["./**/*.ts"],
  "files": ["index.d.ts"],
  "ts-node": {
    "files": true
  }
}
